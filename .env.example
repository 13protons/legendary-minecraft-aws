
# AWS credentials and configuration for Terraform
# All variables below are REQUIRED for automated deployment.


# Your AWS access key for authentication
AWS_ACCESS_KEY_ID=your_aws_access_key_id
# Your AWS secret key for authentication
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
# The AWS region to deploy resources (e.g., us-east-2)
# Must match the region in main.tf (provider "aws" block)
AWS_DEFAULT_REGION=us-east-2

# Path to your public SSH key file (used for EC2 access)
# Should match the path used in main.tf aws_key_pair resource
AWS_SSH_KEY_PATH=~/.ssh/id_rsa.pub

